    "recharts": "^2.12.7",
    "rooks": "^7.14.1",
    "server-only": "^0.0.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "url-join": "^5.0.0",
    "vaul": "^0.9.2",
    "yarn": "^1.22.22",
    "zod": "^3.23.8"
